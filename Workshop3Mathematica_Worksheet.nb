Cell[TextData[{
 "use that  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"r", "\[Rule]", " ", 
    FractionBox["1", 
     RowBox[{"Sin", "[", "\[Theta]", "]"}]]}], TraditionalForm]],
  ExpressionUUID->"64014f35-6998-4aa6-9afe-25757e23f219"],
