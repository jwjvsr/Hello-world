  FormBox[
   RowBox[{"dr", "\[Rule]", " ", 
    RowBox[{
     FractionBox[
      RowBox[{"-", 
       RowBox[{"Cos", "[", "\[Theta]", "]"}]}], 
      SuperscriptBox[
